{
  "name": "frontend",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "pnpm generate:api && remix build",
    "dev": "pnpm generate:api && remix dev --manual",
    "start": "remix-serve ./build/index.js",
    "lint": "eslint ./app",
    "lint:fix": "eslint ./app --fix",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,css,scss,md}\"",
    "format:fix": "prettier --write \"**/*.{js,ts,tsx,css,scss,md}\"",
    "generate:api": "orval --config ./orval.config.cjs",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@faker-js/faker": "^8.1.0",
    "@remix-run/css-bundle": "^2.0.1",
    "@remix-run/node": "^2.0.1",
    "@remix-run/react": "^2.0.1",
    "@remix-run/serve": "^2.0.1",
    "date-fns": "^2.30.0",
    "isbot": "^3.6.8",
    "msw": "^1.3.2",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-typedjson": "^0.2.2",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@remix-run/dev": "^2.0.1",
    "@remix-run/eslint-config": "^2.0.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "orval": "^6.18.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
